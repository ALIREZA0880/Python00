      "source": [
        "\n",
        "def odd_even(num):\n",
        "    result = num % 2\n",
        "    if (result == 0):\n",
        "        print(\"%d is even.\" %(num))\n",
        "    else:\n",
        "        print(\"%d is odd.\" %(num))\n",
        "\n",
        "\n",
        "num = int(input(\"Please Enter A Number: \"))\n",
        "odd_even(num)"
      ]
